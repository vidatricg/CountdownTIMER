   "source": [
    "import time\n",
    "input_time=int(input(\"Enter seconds\"))\n",
    "for x in range(input_time,0,-1):\n",
    "    seconds= x % 60\n",
    "    minutes=int(x/60)%60\n",
    "    hours=int(x/3600)\n",
    "    print(f\"{hours:02}:{minutes:02}:{seconds:02}\")\n",
    "    time.sleep(1)\n",
    "\n",
    "print(\"Time UP\")"
   ]
